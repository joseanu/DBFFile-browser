{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "removeComments": false,
    "sourceMap": true,
    "declaration": true,
    "outDir": "../dist",
    "lib": ["esnext", "dom"],

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  }
}

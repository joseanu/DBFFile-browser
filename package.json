    "url": "https://github.com/yortus/DBFFile/issues"
  },
  "homepage": "https://github.com/yortus/DBFFile",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^12.x",
    "@types/rimraf": "^2.0.2",
    "jest": "^29.7.0",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.5"
  }
